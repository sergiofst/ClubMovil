<configuration>


    <system.web>
        <compilation debug="true" strict="false" explicit="true" targetFramework="4.0" />

        <authentication mode="None">
        </authentication>

        <webServices>
            <conformanceWarnings>
                <remove name='BasicProfile1_1'/>
            </conformanceWarnings>
        </webServices>
        
    </system.web>

    <system.webServer>
        <modules runAllManagedModulesForAllRequests="true"/>
    </system.webServer>
</configuration>
